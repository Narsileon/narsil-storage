{
	"name": "narsil/storage",
	"description": "Narsil Storage",
	"license": "MIT",
	"keywords": [
		"inertia",
		"laravel",
		"narsil",
		"php",
		"storage",
		"react"
	],
	"authors": [
		{
			"name": "Jonathan Rigaux"
		}
	],
	"repositories": [
		{
			"type": "path",
			"url": "../narsil-auth"
		},
		{
			"type": "path",
			"url": "../narsil-forms"
		},
		{
			"type": "path",
			"url": "../narsil-localization"
		},
		{
			"type": "path",
			"url": "../narsil-menus"
		},
		{
			"type": "path",
			"url": "../narsil-policies"
		},
		{
			"type": "path",
			"url": "../narsil-settings"
		},
		{
			"type": "path",
			"url": "../narsil-tables"
		},
		{
			"type": "path",
			"url": "../narsil-tree"
		},
		{
			"type": "path",
			"url": "../narsil-ui"
		}
	],
	"require": {
		"php": "^8.2",
		"laravel/framework": "^11.3",
		"narsil/forms": "dev-main"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Narsil\\Storage\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Narsil\\Storage\\NarsilStorageServiceProvider"
			]
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	}
}
